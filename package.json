{
  "name": "daskeyboard-applet--ups-tracker",
  "version": "1.0.0",
  "publisher": "Das Keyboard",
  "authorName": "Das Keyboard",
  "displayName": "UPS tracker",
  "icon": "assets/icon.png",
  "image": "assets/image.png",
  "licenseUrl": "http://www.gnu.org/licenses/old-licenses/gpl-2.0.txt",
  "changelogUrl": "CHANGELOG.md",
  "readMeEndUserUrl": "README_ENDUSER.md",
  "authorUrl": "https://twitter.com/daskeyboard",
  "readMeUrl": "README.md",
  "issuesUrl": "https://github.com/daskeyboard/daskeyboard-applet--ups-tracker/issues",
  "developerRepoUrl": "https://github.com/daskeyboard/daskeyboard-applet--ups-tracker",
  "homePageUrl": "https://github.com/daskeyboard/daskeyboard-applet--ups-tracker",
  "description": "Track your shippement using color indicators",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "watch": "nodemon .",
    "test": "mocha"
  },
  "engines": {
    "das-keyboard-q": "3.2.2"
  },
  "author": "Das Keyboard",
  "license": "MIT",
  "dependencies": {
    "daskeyboard-applet": "^2.11.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "qConfig": {
    "geometry": {
      "width": 5,
      "height": 1,
      "defaults": {
        "origin": {
          "x": 2,
          "y": 3
        }
      }
    },
    "questions": [
      {
        "key": "trackingNumber",
        "label": "Enter your tracking number from UPS",
        "controlType": "textbox",
        "order": 1,
        "required": true
      },
      {
        "key": "progressColor",
        "label": "Choose the color of the progress bar",
        "controlType": "color",
        "order": 2,
        "value": "#00FF00"
      },
      {
        "key": "backgroundColor",
        "label": "Choose the color of the progress bar background",
        "order": 3,
        "controlType": "color",
        "value": "#FFFF00"
      }
    ]
  }
}