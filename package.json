{
  "name": "daskeyboard-applet--parcel-tracker",
  "version": "1.0.0",
  "publisher": "Das Keyboard",
  "authorName": "Das Keyboard",
  "displayName": "package tracker",
  "icon": "assets/icon.png",
  "image": "assets/image.png",
  "licenseUrl": "http://www.gnu.org/licenses/old-licenses/gpl-2.0.txt",
  "changelogUrl": "CHANGELOG.md",
  "readMeEndUserUrl": "README_ENDUSER.md",
  "authorUrl": "https://twitter.com/daskeyboard",
  "readMeUrl": "README.md",
  "issuesUrl": "https://github.com/daskeyboard/daskeyboard-applet--package-tracker/issues",
  "developerRepoUrl": "https://github.com/daskeyboard/daskeyboard-applet--package-tracker",
  "homePageUrl": "https://github.com/daskeyboard/daskeyboard-applet--package-tracker",
  "description": "Track your shippement using color indicators",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "watch": "nodemon .",
    "test": "mocha"
  },
  "engines": {
    "das-keyboard-q": "3.0.0"
  },
  "author": "Das Keyboard",
  "license": "MIT",
  "dependencies": {
    "daskeyboard-applet": "^2.11.3",
    "track-my-parcel": "^1.0.4"
  },
  "qConfig": {
    "geometry": {
      "width": 5,
      "height": 1,
      "defaults": {
        "origin": {
          "x": 2,
          "y": 3
        }
      }
    },
    "questions": [
      {
        "key": "trackingNumber",
        "label": "Enter your tracking number from ",
        "controlType": "textbox",
        "order": 1,
        "required": true
      },
      {
        "key": "trackingContent",
        "label": "What is being tracked",
        "help": "Example: Sister birthday present",
        "order": 2,
        "controlType": "textbox"
      }
    ]
  },
  "devDependencies": {
    "mocha": "^5.2.0"
  }
}